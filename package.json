{
  "name": "leap",
  "displayName": "Leap",
  "description": "General-purpose motion plugin inspired by leap.nvim",
  "icon": "media/icon.png",
  "version": "0.0.3",
  "publisher": "MurlocCra4ler",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": ["ui", "workspace"],
  "keywords": [
    "leap",
    "leap.nvim",
    "jump",
    "jumpy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MurlocCra4ler/vscode-leap.git"
  },
  "bugs": {
    "url": "https://github.com/MurlocCra4ler/vscode-leap/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "leap.find",
        "title": "Leap Find: Open Widget"
      },
      {
        "command": "leap.findForward",
        "title": "Leap Find: Find Forward"
      },
      {
        "command": "leap.findBackward",
        "title": "Leap Find: Find Backward"
      },
      {
        "command": "leap.match-case",
        "title": "Leap Find: Toggle Match Case"
      }
    ],
    "keybindings": [
      {
        "command": "leap.find",
        "key": "ctrl+alt+f"
      },
      {
        "command": "leap.match-case",
        "key": "alt+c"
      }
    ],
    "configuration": [
      {
        "title": "Leap",
        "properties": {
          "leap.whiteSpacesOnlyMatchNewLine": {
            "type": "boolean",
            "default": false,
            "description": "Determines if <space><space> should only match the new Line symbol like in leap.nvim"
          },
          "leap.customLabels": {
            "type": ["array", "null"],
            "default": null,
            "description": "Use a custom set of jump labels",
            "items": {
              "type": "string"
            }
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
